{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "apimName": {
            "value": "genai-apim-prod"
        },
        "location": {
            "value": "eastus"
        },
        "skuName": {
            "value": "Standard"
        },
        "skuCapacity": {
            "value": 2
        },
        "publisherEmail": {
            "value": "platform-admin@example.com"
        },
        "publisherName": {
            "value": "GenAI Platform"
        },
        "backendUrl": {
            "value": "https://genai-orchestrator.azurewebsites.net"
        },
        "appInsightsId": {
            "value": "/subscriptions/<subscription-id>/resourceGroups/<rg-name>/providers/microsoft.insights/components/genai-appinsights-prod"
        },
        "appInsightsKey": {
            "value": "<instrumentation-key>"
        },
        "tags": {
            "value": {
                "environment": "prod",
                "project": "genai-platform",
                "costCenter": "production"
            }
        }
    }
}